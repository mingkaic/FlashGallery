extends layout

block content
	#header
		h1= title
		p Welcome to #{title}
	#content
		form(action="/uploads", method="post", enctype="multipart/form-data")
			div
				input(type="file", name="displayImage")
			div
				input(type="submit")
		each Url in imageUrl
			img(src= Url)
	#footer
		p Ming Kai Chen